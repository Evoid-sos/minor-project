<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_bg3"
    android:orientation="vertical"
    tools:context=".HelpLocation">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="347dp"
        android:layout_height="285dp"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.5"
        map:layout_constraintStart_toStartOf="parent"
        tools:context="GetNearbyPlaces.HelpLocation" />

    <ImageView
        android:id="@+id/policeHelp"
        android:layout_width="106dp"
        android:layout_height="102dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="242dp"
        android:src="@drawable/policehelp"
        map:layout_constraintBottom_toTopOf="@+id/map"
        map:layout_constraintEnd_toStartOf="@+id/hospitalHelp"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="parent"
        android:onClick="onClick"
        />

    <ImageView
        android:id="@+id/hospitalHelp"
        android:layout_width="106dp"
        android:layout_height="102dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="242dp"
        map:layout_constraintBottom_toTopOf="@+id/map"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintStart_toEndOf="@+id/policeHelp"
        map:layout_constraintTop_toTopOf="parent"
        map:srcCompat="@drawable/hospital"
        android:onClick="onClick"
        />

    <ImageView
        android:id="@+id/fireHelp"
        android:layout_width="106dp"
        android:layout_height="102dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="109dp"
        map:layout_constraintBottom_toTopOf="@+id/map"
        map:layout_constraintEnd_toStartOf="@+id/oldageHelp"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/policeHelp"
        map:srcCompat="@drawable/firehelp"
        android:onClick="onClick"
        />

    <ImageView
        android:id="@+id/oldageHelp"
        android:layout_width="106dp"
        android:layout_height="102dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="109dp"
        map:layout_constraintBottom_toTopOf="@+id/map"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintStart_toEndOf="@+id/fireHelp"
        map:layout_constraintTop_toBottomOf="@+id/hospitalHelp"
        map:srcCompat="@drawable/oldagehelp"
        android:onClick="onClick"
        />

</androidx.constraintlayout.widget.ConstraintLayout>